<Window x:Class="PortfolioTrading.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Button Name="btnLaunch" Click="btnLaunch_Click">Launch</Button>
            <Button Name="btnExit" Click="btnExit_Click">Exit</Button>
            <Button Name="btnConnect" Click="btnConnect_Click">Connect</Button>
            <Button Name="btnQuoteConn" Click="btnQuoteConn_Click">Quote Connect</Button>
        </StackPanel>
    </Grid>
</Window>
